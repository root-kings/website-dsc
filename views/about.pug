	//- Page Title
	section.page-title(style="background-image:url(images/background/2.jpg);")
		.auto-container
			//- span.float-text About Us
			h1 About Developer Student Club GHRCE
			//- ul.bread-crumb.clearfix
			//- 	li
			//- 		a(href="index-2.html") Home 
			//- 	li About
	//- End Page Title
	//- About Section
	section.about-section
		.anim-icons
			span.icon.icon-cross-1
			span.icon.icon-circle-9
		//- span.float-text About The
		.auto-container
			//- .sec-title
				h2 About The Theme
			.about-carousel.owl-carousel.owl-theme
				//- Slide Item
				.slide-item.clearfix
					.image-box
						figure.image
							img(src="images/resource/about-img-1.jpg" alt="")
					.content-box
						.inner-box
							span.title Diversity
							h3 Welcoming girls
							.text
								| Pixel Pioneers, a one-day conference for web and UX designers and front-end developers, will take place on the 23rd of November 2019 in Belfast.
							.btn-box
								a.theme-btn.btn-style-one(href="contact.html") Join Now
				//- Slide Item
				//- .slide-item.clearfix
					.image-box
						figure.image
							img(src="images/resource/about-img-2.jpg" alt="")
					.content-box
						.inner-box
							span.title Research & Concept
							h3 Pixel Pioneers Belfast 2019
							.text
								| Pixel Pioneers, a one-day conference for web and UX designers and front-end developers, will take place on the 23rd of November 2019 in Belfast.
							.btn-box
								a.theme-btn.btn-style-one(href="contact.html") Register Now
	//- End About Section
	//- Fun Fact Section
	section.fun-fact-section-two
		.anim-icons
			span.icon.icon-circle-10
			span.icon.icon-twist-line-3
		.auto-container
			.fact-counter
				.row.clearfix
					//- Column
					.counter-column.col-lg-3.col-md-6.col-sm-12.wow.fadeInUp
						.count-box
							span.icon.flaticon-mug
							span.count-text(data-speed="3000" data-stop="70") 0
							| +
							h4.counter-title
								| Usefull 
								br
								| Sessions
					//- Column
					.counter-column.col-lg-3.col-md-6.col-sm-12.wow.fadeInUp(data-wow-delay="400ms")
						.count-box
							span.icon.flaticon-launch
							span.count-text(data-speed="3000" data-stop="10") 0
							| +
							h4.counter-title
								| Technologies
					//- Column
					.counter-column.col-lg-3.col-md-6.col-sm-12.wow.fadeInUp(data-wow-delay="800ms")
						.count-box
							span.icon.flaticon-lady
							span.count-text(data-speed="3000" data-stop="6") 0
							| k+
							h4.counter-title
								| Regular 
								br
								| Crowd
					//- Column
					.counter-column.col-lg-3.col-md-6.col-sm-12.wow.fadeInUp(data-wow-delay="1200ms")
						.count-box
							span.icon.flaticon-support-1
							span.count-text(data-speed="3000" data-stop="35") 0
							| +
							h4.counter-title
								| Visionary 
								br
								| Speakers
	//- End Fun Fact Section
